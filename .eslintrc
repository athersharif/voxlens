{
    "plugins": [],
    "extends": ["eslint:recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 6
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "amd": true
    },
    "rules": {
      "quotes": [2, "single", { "avoidEscape": true }],
      "no-console": 0
    },
  }
  